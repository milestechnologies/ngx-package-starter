{
    "name": "ngx-package-starter",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "npm run build && ng serve",
        "build": "ng-packagr -p src/package.json",
        "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
        "test": "ng test --karma-config karma.conf.js --browsers ChromeHeadlessNoSandbox --watch=false --code-coverage",
        "postversion": "git push --follow-tags"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/milestechnologies/ngx-package-starter"
    },
    "author": {
        "name": "milesdevservices",
        "email": "devservices@milestechnologies.com"
    },
    "keywords": [
        "angular"
    ],
    "prettier": {
        "trailingComma": "all",
        "singleQuote": true,
        "tabWidth": 4,
        "arrowParens": "always"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/milestechnologies/ngx-package-starter/issues"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1100.1",
        "@angular/animations": "^11.0.0",
        "@angular/cli": "^11.0.1",
        "@angular/common": "^11.0.0",
        "@angular/compiler": "^11.0.0",
        "@angular/compiler-cli": "^11.0.0",
        "@angular/core": "^11.0.0",
        "@angular/forms": "^11.0.0",
        "@angular/platform-browser": "^11.0.0",
        "@angular/platform-browser-dynamic": "^11.0.0",
        "@angular/router": "^11.0.0",
        "@types/jasmine": "3.6.1",
        "bootstrap": "^4.5.3",
        "classlist.js": "^1.1.20150312",
        "codelyzer": "6.0.1",
        "font-awesome": "^4.7.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "^5.2.3",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "karma-junit-reporter": "^2.0.1",
        "karma-sourcemap-loader": "^0.3.8",
        "karma-spec-reporter": "0.0.32",
        "ng-packagr": "^11.0.2",
        "rxjs": "^6.6.3",
        "tslib": "^2.0.3",
        "tslint": "~5.7.0",
        "typescript": "~4.0.5",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.11.3"
    },
    "engines": {
        "node": ">=6.0.0"
    }
}
